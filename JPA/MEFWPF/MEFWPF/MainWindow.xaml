<Window x:Class="MEFWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MEFWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <WrapPanel DockPanel.Dock="Left">
            <TextBox Width="100"></TextBox>
            <TextBox Width="100"></TextBox>
        </WrapPanel>
        <StackPanel DockPanel.Dock="Left">
            <Button Height="50" Width="100">Suma</Button>
            <Button Height="50" Width="100">Iloczyn</Button>
        </StackPanel>
        <TextBlock Width="100" Height="100" DockPanel.Dock="Left"></TextBlock>
    </DockPanel>
</Window>
