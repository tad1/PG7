<UserControl x:Class="FieldCelluarAutomaton.Views.CellInfo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:FieldCelluarAutomaton.Views"
             mc:Ignorable="d">
    <StackPanel>
        <WrapPanel>
            <TextBlock Text="Position X,Y"></TextBlock>
        </WrapPanel>
        <WrapPanel>
            <TextBlock Text="Real:"></TextBlock>
            <TextBox Width="40"></TextBox>
        </WrapPanel>
        <WrapPanel>
            <TextBlock Text="Imaginary:"></TextBlock>
            <TextBox Width="40"></TextBox>
        </WrapPanel>
    </StackPanel>
</UserControl>
